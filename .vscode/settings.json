// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {
            //"when": "$(basename).ts"
        },
        "**/*.d.ts":true,
        "**/*.js.map": true,
        "**/node_modules/": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/modules.json"
            ],
            "url": "./schema/modules.json"
        },
        {
            "fileMatch": [
                "/executors.json"
            ],
            "url": "./schema/executors.json"
        },
        {
            "fileMatch": [
                "/kanro.json"
            ],
            "url": "./schema/kanro.json"
        },
        {
            "fileMatch": [
                "/services.json"
            ],
            "url": "./schema/services.json"
        }
    ],
    "vsicons.presets.angular": false
}